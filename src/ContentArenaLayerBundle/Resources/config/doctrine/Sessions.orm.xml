<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ContentArenaLayerBundle\Entity\Sessions" table="sessions">
    <indexes>
      <index name="pages_id" columns="pages_id"/>
      <index name="user_id" columns="user_id"/>
      <index name="ts" columns="ts"/>
    </indexes>
    <id name="id" type="string" column="id" length="32">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="pagesId" type="integer" column="pages_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
      </options>
    </field>
    <field name="data" type="text" column="data" length="16777215" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ts" type="datetime" column="ts" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="ip" type="integer" column="ip" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="ua" type="string" column="ua" length="250" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
