<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ContentArenaLayerBundle\Entity\Pages" table="pages">
    <indexes>
      <index name="parent_id" columns="parent_id"/>
      <index name="templates_id" columns="templates_id"/>
      <index name="modified" columns="modified"/>
      <index name="created" columns="created"/>
      <index name="status" columns="status"/>
      <index name="published" columns="published"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="name_parent_id" columns="name,parent_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="parentId" type="integer" column="parent_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="templatesId" type="integer" column="templates_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
    <field name="name" type="string" column="name" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="integer" column="status" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">1</option>
      </options>
    </field>
    <field name="modified" type="datetime" column="modified" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
    <field name="modifiedUsersId" type="integer" column="modified_users_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">2</option>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">2015-12-18 00:09:00</option>
      </options>
    </field>
    <field name="createdUsersId" type="integer" column="created_users_id" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">2</option>
      </options>
    </field>
    <field name="published" type="datetime" column="published" nullable="true"/>
    <field name="sort" type="integer" column="sort" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
